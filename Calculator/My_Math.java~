class My_Math
{
  public static int factorial(int num)
  {
    if(num==0)
      return 1;
    else{
      return factorial(num-1)*num;
    }
  }
  
  public static int fibonacci(int num)
  {
    if(num==0 || num==1)
      return 1;
    else{
      return fibonacci(num-1)+fibonacci(num-2);
    }
  }
}